<h2>Update an Post</h2>

<%= form_with model: @post, method: :patch, local: true do |form| %>
  <%= form.label :title%>
  <%= form.text_field :title %>
  <br>
  <br>
  <%= form.label :content%>
  <%= form.text_field :content%>
  <br>
  <br>
  <%= form.submit 'Edit Post'%>
<% end %>
<br>
<br>
<h4><%= link_to 'Back Home', posts_path %></h4>
